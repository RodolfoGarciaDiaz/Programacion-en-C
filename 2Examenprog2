#include <stdio.h>

int main (void){
    float Num[5];
    int a,b,c,d,e;
    int *dir_a,*dir_b,*dir_c,*dir_d,*dir_e;

    printf("Ingrese 5 numeros\n");

    for (int i=0; i<5; i++){
    printf("Ingrese el numero %d: ",i+1);
    scanf("%d",&Num[i]);
    }
    Num[0]=a;
    Num[1]=b;
    Num[2]=c;
    Num[3]=d;
    Num[4]=e;

    printf("\nEl valor de a=%d",a);
    printf("\nEl valor de b=%d",b);
    printf("\nEl valor de c=%d",c);
    printf("\nEl valor de d=%d",d);
    printf("\nEl valor de e=%d",e);

    dir_a=&a;
    printf("\nLa direccion de a es=%d",&a);
    dir_b=&b;
    printf("\nLa direccion de b es=%d",&b);
    dir_c=&c;
    printf("\nLa direccion de c es=%d",&c);
    dir_d=&d;
    printf("\nLa direccion de d es=%d",&d);
    dir_e=&e;
    printf("\nLa direccion de e es=%d",&e);
return 0;
}
