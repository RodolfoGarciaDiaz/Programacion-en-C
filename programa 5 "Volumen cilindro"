#include <stdio.h>
#include <math.h>
int main()
{
    int r,h;
    float Vol;

    printf ("Programa calcula volumen de un cilindro(cm)\n");
   printf ("Indica el valor del radio de la base\n");
   scanf ("%d",&r);
   printf ("Indica el valor de la altura del cilindro\n");
   scanf ("%d",&h);

Vol=(pow(r,2)*3.1416)*h;

printf("El Volumen del cilindro es:%1.2f",Vol); printf ("cm3\n");

   return 0;
}
