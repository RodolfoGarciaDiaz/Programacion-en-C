#include <stdio.h>
#include<math.h> 
#include<stdio.h>

int main(void){
	printf("Programa que calcula la media o el promedio de 30 calificaciones \n");
	float calificaciones[30]={22,19,18,22,19,20,22,21,19,20,22,24,30,36,22,20,19,18,16,19,20,22,21,19,20,24,14,30,25,20};
	float promedio = 0; 
	for (int i=0; i<30; i++){
		promedio=promedio+calificaciones[i];
	}
	printf("el promedio de las calificaciones es: %.2f\n",promedio/30);
	return 0;
}
