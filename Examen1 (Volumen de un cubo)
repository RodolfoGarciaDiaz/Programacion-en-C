#include <stdio.h>
 int main(void){
  float L;
 	float Volumen;
 	printf("Programa volumen de un cubo.\n");
 	printf("Indica la magnitud de uno de los lados de la base (cm)=");
 	scanf("%f",&L);
 	Volumen=L*L*L;
 	printf("El volumen de su cubo es: %1.2f cm3 \n",Volumen);
 	return 0;
 }
